{
  "name": "demo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "test": "mocha --exit",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all --config 'config/config.json' --env local --debug",
    "clear-seed": "npx sequelize-cli db:seed:undo:all --config 'config/config.json' --env local",
    "init-db": "node -e 'require(\"./models\").sequelize.sync();'",
    "drop-db": "node -e 'require(\"./models\").sequelize.drop();'",
    "reset-db": "npm run drop-db && npm run init-db && npm run seed"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.18.2",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "~1.4.3",
    "cookie-session": "^1.4.0",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-session": "^1.17.2",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pug": "2.0.0-beta11",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "mocha": "^5.1.1",
    "supertest": "^3.0.0",
    "chai": "^4.3.4",
    "mariadb": "^2.5.4"
  }
}
